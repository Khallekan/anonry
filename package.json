{
  "name": "anonry",
  "version": "0.0.1",
  "description": "anonymous application to write down thoughts",
  "repository": "https://github.com/Khallekan/anonry.git",
  "author": "khallekan <okeolalekanisaac@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "start": "node dist/server/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q src/server/index.ts\"",
    "test": "jest",
    "lint": "eslint",
    "lint:fix": "eslint . --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "googleapis": "^113.0.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^4.10.0",
    "mongoose": "^7.0.1",
    "mongoose-paginate-v2": "^1.7.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.15.0",
    "@types/node-cron": "^3.0.4",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/pug": "^2.0.6",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.2.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "jest-environment-node": "^29.2.0",
    "lint-staged": "^13.2.0",
    "mongodb-memory-server": "^8.9.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.4",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier src -w"
    ]
  }
}
